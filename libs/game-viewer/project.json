    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/game-viewer/**/*.ts",
          "libs/game-viewer/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
