{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "game-viewer",
  "projectType": "library",
  "sourceRoot": "libs/game-viewer/src",
  "prefix": "knights-rpggame-web",
  "targets": {
    "test": {
